# Frontend Environment Configuration (Vite)
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:5000
VITE_SOCKET_URL=http://localhost:5000

# OpenAI Configuration (for AI Chatbot)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# App Configuration
VITE_APP_NAME=MindBot - Mental Health Support
VITE_APP_VERSION=1.0.0

# Development Settings
GENERATE_SOURCEMAP=true
VITE_DEBUG=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Crisis Hotline Numbers (Customize for your region)
VITE_CRISIS_HOTLINE_NATIONAL=988
VITE_CRISIS_HOTLINE_EMERGENCY=911
VITE_CRISIS_HOTLINE_TEXT=741741
